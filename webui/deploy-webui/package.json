{
  "name": "@rxdi/deploy-webui",
  "version": "0.7.38",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "generate": "gapi schema introspect --collect-documents --collect-types && npm run generate:tschema",
    "generate:tschema": "./node_modules/.bin/apollo-codegen generate ./src/**/*.graphql --schema ./src/app/core/api-introspection/schema.json --target typescript --output ./src/app/core/api-introspection/operation-result-types.ts",
    "build": "ng build --prod --aot --base-href /webui/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rxdi/deploy-webui"
  },
  "private": true,
  "dependencies": {
